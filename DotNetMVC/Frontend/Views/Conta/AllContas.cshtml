@model IEnumerable<Conta>

<h1>Lista de todas as contas</h1>
<hr>
<table class="table table-striped">
    <thead>
        <th scope="col">Número da Conta</th>
        <th scope="col">Agência </th>
        <th scope="col">Id Tipo</th>
        <th scope="col">Nome Conta</th>
        <th scope="col">Saldo</th>
        <th scope="col">Código</th>
        <th scope="col">Titular</th>
        <th scope="col">CPF</th>
        <th scope="col">Telefone</th>
        <th scope="col">Email</th>
    </thead>

    <tbody>
        @foreach (Conta contas in Model)
        {
            <tr>
                <td>@contas.numConta</td>
                <td>@contas.agencia</td>
                <td>@contas.tipoConta</td>
                <td>@contas.nomeTipoConta</td>
                <td>@contas.saldo</td>
                <td>@contas.codInt</td>
                <td>@contas.nome</td>
                <td>@contas.cpf</td>
                <td>@contas.telefone</td>
                <td>@contas.email</td>
            </tr>
        }
    </tbody>
</table>